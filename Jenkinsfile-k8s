pipeline {
  agent any
    stages {
      stage('Build') {
        steps {
          sh 'mvn -B -DskipTests clean package'
        }
      }
      stage('K8s') {
        steps {
          sh 'kubectl set image deployments/hello-node 79f03256b92b24aa2e32cc729328892f5d903cb09766292a27c188ca64503211=evaanli/teedy_jenkins'
        }
      }
  }
}
